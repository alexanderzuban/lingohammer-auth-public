<?xml version="1.0" encoding="UTF-8" ?>
<Application
    x:Class="LingoHammer.App"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LingoHammer">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
                <ResourceDictionary Source="Resources/Styles/Strings.xaml" />
                <ResourceDictionary Source="Resources/Styles/Defaults.xaml" />
                <ResourceDictionary Source="Resources/Styles/Icons.xaml" />
                <ResourceDictionary Source="Resources/Styles/Controls.xaml" />
                <ResourceDictionary Source="Resources/Styles/Converters.xaml" />
                <ResourceDictionary Source="Resources/Styles/NoticeView.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
